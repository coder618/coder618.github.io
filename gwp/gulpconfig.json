{
	"browserSyncOptions": {
		"proxy": "localhost:8080/",
		"notify": false
	},
	"browserSyncWatchFiles": ["./css/*.min.css", "./js/*.min.js", "./**/*.php"],
	"paths": {
		"js": "./dist/js",
		"css": "./dist/css",
		"img": "./dist/img",
		"imgsrc": "./source_code/img",
		"sass": "./source_code/scss",
		"node": "./node_modules/",
		"bower": "./bower_components/",
		"dev": "./source_code",
		"dist": "./dist",
		"distprod": "./dist-product",
		"js_packages_list": ["./source_code/js/custom.js"],

		"vendor": ""
	}
}
